{
  "manifest_version": 2,
  "name": "The Website is Down",
  "version": "1.0",
  "description": "For Logging Internet Loss",
  "content_scripts": [ {
    "js": ["jquery-1.8.2.min.js", "index.js"],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "browser_action": {
    "default_title": "The Website is Down",
    "default_popup": "index.html"
  },

  "background": {
    "scripts": ["background.js"]
  }
}
